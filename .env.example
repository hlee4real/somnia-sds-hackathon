# Somnia Network Configuration
RPC_URL=https://api.infra.mainnet.somnia.network/
NEXT_PUBLIC_RPC_URL=https://api.infra.mainnet.somnia.network/

# Wallet Configuration
PRIVATE_KEY=your_private_key_here
WALLET_ADDRESS=0x_your_wallet_address_here
NEXT_PUBLIC_WALLET_ADDRESS=0x_your_wallet_address_here

# Publisher Configuration (address that updates prices)
PUBLISHER_ADDRESS=0x_your_publisher_address_here
NEXT_PUBLIC_PUBLISHER_ADDRESS=0x_your_publisher_address_here

# Schema ID (set after deploying schema)
PRICE_SCHEMA_ID=0x_your_schema_id_here
NEXT_PUBLIC_PRICE_SCHEMA_ID=0x_your_schema_id_here

# CoinMarketCap API Key
# Get free key from: https://coinmarketcap.com/api/
CMC_API_KEY=your_coinmarketcap_api_key_here

# WalletConnect Project ID (Optional)
# Get free project ID from: https://cloud.walletconnect.com
# Required for WalletConnect integration in the Connect Wallet button
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here
